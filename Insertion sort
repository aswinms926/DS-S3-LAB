#include<stdio.h>
void main()
{
   int n,a[100],i,j,t=0;
   printf("Enter the lim : ");
   scanf("%d",&n);
      printf("Enter the elements : ");

   for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);
   }
   i=1;
   while(i<n)
   {
    j=i;
     while(a[j]<a[j-1])
     {
         t=a[j];
         a[j]=a[j-1];
         a[j-1]=t;
         j=j-1;
     }
     i=i+1;
   }
  
   printf("The array after insertionsort is \n");
   for(i=0;i<n;i++)
   {
     printf("%d\t",a[i]);
   }
   printf("\n");
}
/*****************************************************************************
OUTPUT

Enter the lim : 5
Enter the elements : 8
1
6
4
2
The array after insertionsort is 
1	2	4	6	8	
*****************************************************************************/
