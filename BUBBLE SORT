
#include <stdio.h>

int main()
{
    int i,n,c=0,temp,j;
    c++;
    printf("Enter the array size  : ");
    scanf("%d",&n);
    int a[n];
        printf("Enter the array elements  : ");
    for(i=0;i<n;i++)
    {
         scanf("%d",&a[i]);
         c++;
         c++;
    }c++;
    for(i=0;i<n-1;i++)
    {
        c++;
        for(j=0;j<n-i-1;j++)
        {
            c++;
            if(a[j]>a[j+1])
            {
                c++;
                c++;
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }c++;
    }c++;
    printf("sorted array is ");
    for(i=0;i<n;i++)
    {
         printf("%d ",a[i]);
         c++;
         c++;
    }c++;
    
    
     
    printf("\ntime complexity is %d",c);
    printf("\nspace complexity is %d",20+4*n);
    return 0;
}
